<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="Arte-E-Magia_tstPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>entidades.Despesas</class>
    <class>entidades.Telefone</class>
    <class>entidades.Cliente</class>
    <class>entidades.Crianca</class>
    <class>entidades.Funcionario</class>
    <class>entidades.Email</class>
    <class>entidades.Enderecos</class>
    <class>entidades.Festa</class>
    <class>entidades.Pacote</class>
    <class>entidades.Usuario</class>
    <class>entidades.FormaPagamento</class>
    <class>entidades.ConfigNotificacaoAniversariante</class>
    <class>entidades.DespesaFesta</class>
    <class>entidades.FestaCrianca</class>
    <class>entidades.FuncionarioFesta</class>
    <class>entidades.HorarioFesta</class>
    <class>entidades.PacotesAdicionaisFesta</class>
    <class>entidades.PagamentoAdiantadoFesta</class>
    <class>entidades.PagamentoDespesasDetalhe</class>
    <class>entidades.PagamentoFesta</class>
    <class>entidades.TabelaAgendamentoFesta</class>
    <class>entidades.TipoDeDespesa</class>
    <class>entidades.TipoPacoteAdicional</class>
    <class>entidades.ValorAdicionalFesta</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/arteemagia"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.password" value=""/>
    </properties>
  </persistence-unit>
</persistence>
